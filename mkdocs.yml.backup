site_name: 'My Documentation'
site_description: 'My GitHub Pages Documentation'
nav:
  - Home: _index.md
  - Getting Started: getting_started/_index.md
  - Infrastructure: infrastructure/_index.md
  - Kubernetes: kubernetes/_index.md
  - O11y: o11y/_index.md
  - Technical Solutions: technical_solutions/_index.md
  - Toolchain: toolchain/_index.md
  - Tutorials: tutorials/_index.md
repo_url: https://github.com/khngu/khngu.github.io/tree/main/docs
edit_uri: edit/main/docs
plugins:
  - techdocs-core
  - search
markdown_extensions:
  - admonition
  - toc:
      permalink: true
  - attr_list
  - md_in_html
  - pymdownx.superfences
  - pymdownx.tabbed:
      alternate_style: true
extra_css:
  - extra.css
extra_javascript:
  - extra.js
theme:
  name: material
  palette:
    # - scheme: default
    #   primary: red
    #   accent: red
    # - scheme: slate
    #   primary: indigo
    #   accent: indigo
  favicon: assets/favicon.png
  features:
    - navigation.tabs
    - navigation.sections
    - toc.integrate
